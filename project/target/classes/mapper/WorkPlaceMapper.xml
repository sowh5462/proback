<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.WorkPlaceMapper">
	<select id="workplace" resultType="com.example.domain.WorkplaceVO">
		select * from tbl_workplace where use_id=#{use_id};
	</select>
	
	<select id="workabout" resultType="com.example.domain.WorkplaceVO">
		select * from tbl_workplace where use_work_num = #{use_work_num}
	</select>
	
	
	
	
</mapper>